{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/vue-starter-kit",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-exif": "*",
        "ext-gd": "*",
        "artesaos/seotools": "^1.3",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "glorand/laravel-model-settings": "^8.0",
        "hashids/hashids": "^5.0",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^2.0",
        "intervention/image-laravel": "^1.5",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.33",
        "laravel/nightwatch": "^1.7",
        "laravel/octane": "^2.10",
        "laravel/sanctum": "^4.1",
        "laravel/scout": "^10.15",
        "laravel/tinker": "^2.10.1",
        "lorisleiva/laravel-actions": "^2.9",
        "meilisearch/meilisearch-php": "^1.15",
        "nativephp/electron": "^1.1",
        "pirsch-analytics/laravel-pirsch": "^1.0",
        "spatie/laravel-data": "^4.17",
        "spatie/laravel-medialibrary": "^11.13",
        "spatie/laravel-passkeys": "^1.0",
        "spatie/laravel-permission": "^6.20",
        "spatie/laravel-sitemap": "^7.3",
        "spatie/laravel-sluggable": "^3.7",
        "spatie/robots-txt": "^2.5",
        "symfony/http-client": "^7.3",
        "symfony/mailgun-mailer": "^7.3",
        "tightenco/ziggy": "^2.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "barryvdh/laravel-ide-helper": "^3.5",
        "captenmasin/laravel-dumper": "^1.0",
        "fakerphp/faker": "^1.23",
        "laravel/dusk": "^8.3",
        "laravel/pail": "^1.2",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "pestphp/pest-plugin-stressless": "^3.1",
        "pestphp/pest-plugin-watch": "^3.0",
        "soloterm/solo": "^0.5.0",
        "spatie/laravel-ray": "^1.40"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/captenmasin/laravel-dumper"
        },
        {
            "type": "vcs",
            "url": "https://github.com/captenmasin/laravel-open-library.git"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "dev:ssr": [
            "npm run build:ssr",
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"php artisan inertia:start-ssr\" --names=server,queue,logs,ssr --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "native:dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -k -c \"#93c5fd,#c4b5fd\" \"php artisan native:serve --no-interaction\" \"npm run dev\" --names=app,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
